<!DOCTYPE HTML>
<html>
<head>
	<script type="text/javascript">
		window.onload = function () {
                        CanvasJS.addColorSet("blueShades",
                                [//colorSet Array
                                "#008080",
                                "#008B8B"
                                
                                 
                                ]);
			var chart = new CanvasJS.Chart("chartContainer", {
				colorSet: "blueShades",
        animationEnabled: true, 
		    animationDuration: 2000,
                                
                                title: {
					text: "Busy Hours"
				},
				data: [{
					type: "column",
					dataPoints: [
				  { x: 0, y: 0 },
					{ x: 1, y: 0 },
					{ x: 2, y: 0 },
					{ x: 3, y: 0 },
					{ x: 4, y: 0 },
					{ x: 5, y: 0 },
					{ x: 6, y: 0 },
					{ x: 7, y: 0 },
					{ x: 8, y: 0 },
					{ x: 9, y: 0 },
					{ x: 10, y: 3 },
          { x: 11, y: 7 },
          { x: 12, y: 10 },
					{ x: 13, y: 12 },
					{ x: 14, y: 10 },
					{ x: 15, y: 8 },
					{ x: 16, y: 6 },
					{ x: 17, y: 9 },
					{ x: 18, y: 13 },
					{ x: 19, y: 15 },
					{ x: 20, y: 12 },
					{ x: 21, y: 8 },
					{ x: 22, y: 3 },
          { x: 23, y: 0 },
					]
				}]
			});
			chart.render();
		}
	</script>
	<script src="../../canvasjs.min.js"></script>
	<title>CanvasJS Example</title>
</head>

<body>
	<div id="chartContainer" style="height: 400px; width: 100%;"></div>
</body>

</html>
